{
  "name": "dunder-mifflin",
  "private": true,
  "packageManager": "pnpm@10.4.0",
  "scripts": {
    "dev:portal": "pnpm --filter @dunder/portal dev",
    "build:portal": "pnpm --filter @dunder/portal build",
    "test:portal": "pnpm --filter @dunder/portal test",
    "test:e2e:pr2": "playwright test --config tests/e2e/config/playwright.config.ts tests/e2e/specs/auth-sales-navigation.spec.ts",
    "test:e2e:pr3": "playwright test --config tests/e2e/config/playwright.config.ts tests/e2e/specs/sales-order-placement.spec.ts",
    "test:e2e:pr4": "playwright test --config tests/e2e/config/playwright.config.ts tests/e2e/specs/warehouse-dispatch.spec.ts",
    "test:e2e:smoke": "playwright test --config tests/e2e/config/playwright.config.ts tests/e2e/specs/auth-sales-navigation.spec.ts tests/e2e/specs/sales-order-placement.spec.ts tests/e2e/specs/warehouse-dispatch.spec.ts"
  },
  "devDependencies": {
    "@playwright/test": "1.51.1"
  }
}
